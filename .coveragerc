[run]
source = gitlab_depcheck
omit =
    */tests/*
    */__pycache__/*
    */site-packages/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
precision = 2

[html]
directory = htmlcov
